    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "gitmoji-cli": "^9.7.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
