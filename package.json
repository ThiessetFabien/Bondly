{
  "name": "relational-partner-manager-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "oxlint src/",
    "lint:fix": "oxlint src/ --fix",
    "lint:next": "next lint --fix",
    "lint:dead-code": "oxlint src/ --deny-warnings",
    "lint:imports": "oxlint src/ --deny-warnings",
    "lint:comprehensive": "oxlint src/ --comprehensive",
    "lint:report": "oxlint src/ --format=json --output-file=lint-report.json",
    "analyze:deps": "depcheck --ignores='@types/*,prettier,eslint,typescript,@tailwindcss/postcss,commitlint-config-gitmoji,jest-environment-jsdom,lint-staged'",
    "analyze:unused-exports": "ts-unused-exports tsconfig.json",
    "analyze:dead-code": "unimported --init && unimported",
    "analyze:comprehensive": "pnpm run analyze:deps && pnpm run analyze:unused-exports && pnpm run lint:dead-code",
    "analyze:full": "node scripts/analyze-dead-code.js",
    "optimize:build": "pnpm run lint:fix && pnpm run build",
    "validate:sidebar": "./scripts/validate-sidebar-structure.sh",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:api": "node scripts/test-api-json.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:simple": "playwright test --config=playwright.config.simple.ts",
    "test:e2e:simple:ui": "playwright test --config=playwright.config.simple.ts --ui",
    "test:e2e:basic": "playwright test --config=playwright.config.simple.ts e2e/basic-app.spec.ts",
    "playwright:install": "playwright install",
    "playwright:quick-test": "node scripts/playwright/quick-test.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "commit": "gitmoji -c",
    "commit:help": "./scripts/commit-help.sh",
    "commit:test": "./scripts/test-commit-format.sh",
    "commitlint": "commitlint --edit",
    "validate-commits": "commitlint --from=HEAD~1 --to=HEAD --verbose",
    "prepare": "husky",
    "docker:help": "./scripts/docker-help.sh",
    "docker:setup": "./scripts/setup-docker.sh",
    "docker:build": "./scripts/docker-help.sh build",
    "docker:build-dev": "./scripts/docker-help.sh build-dev",
    "docker:run": "./scripts/docker-help.sh run",
    "docker:dev": "./scripts/docker-help.sh dev",
    "docker:stop": "./scripts/docker-help.sh stop",
    "docker:clean": "./scripts/docker-help.sh clean",
    "docker:logs": "./scripts/docker-help.sh logs",
    "docker:shell": "./scripts/docker-help.sh shell",
    "docker:security": "./scripts/docker-security-check.sh",
    "docker:test": "./scripts/docker-test.sh"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.525.0",
    "next": "15.3.4",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/ui": "^3.2.4",
    "commitlint-config-gitmoji": "^2.3.1",
    "daisyui": "^5.0.50",
    "depcheck": "^1.4.7",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.3.4",
    "gitmoji-cli": "^9.7.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "oxlint": "^1.9.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "ts-unused-exports": "^11.0.1",
    "typescript": "^5.8.3",
    "unimported": "^1.31.1",
    "vitest": "^3.2.4"
  }
}
