{
  "name": "relational-partner-manager-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "oxlint src/",
    "lint:fix": "oxlint src/ --fix",
    "lint:next": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"Tests not configured yet\" && exit 0",
